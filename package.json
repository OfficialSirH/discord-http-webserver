{
  "name": "@sirh/discord-http-webserver",
  "version": "0.1.0",
  "description": "Library for Discord HTTP Interactions",
  "main": "dist/src/index.js",
  "module": "dist/src/index.mjs",
  "typings": "dist/src/index.d.ts",
  "author": "SirH",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "dependencies": {
    "@discordjs/builders": "^1.7.0",
    "@discordjs/collection": "^2.0.0",
    "@discordjs/core": "^1.1.1",
    "@discordjs/rest": "^2.2.0",
    "@sapphire/result": "^2.6.6",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "form-data-encoder": "^4.0.2",
    "tweetnacl": "^1.0.3",
    "undici": "^5.28.4"
  },
  "scripts": {
    "test": "vitest",
    "cleanup": "rm -rf dist",
    "build": "tsc --noEmit && tsup",
    "setup": "yarn run format",
    "format": "prettier --write . && eslint --fix src",
    "lint": "eslint --ext .ts src",
    "changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml",
    "release": "cliff-jumper"
  },
  "exports": {
    ".": {
      "node": {
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "import": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.mjs"
        }
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "discord",
    "api",
    "http",
    "interactions",
    "application",
    "node",
    "discordapp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficialSirH/discord-http-webserver.git"
  },
  "bugs": {
    "url": "https://github.com/OfficialSirH/discord-http-webserver/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@favware/cliff-jumper": "^3.0.2",
    "@favware/npm-deprecate": "^1.0.7",
    "@types/node": "^18.19.31",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitest/coverage-v8": "^1.5.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.1.1"
}
